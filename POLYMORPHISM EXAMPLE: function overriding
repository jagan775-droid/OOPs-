// POLYMORPHISM EXAMPLE: function overriding
#include <iostream>
#include <string>
using namespace std;
class parent{
    public:
    void getInfo(){
        cout<<"Parent class \n";
    }
};
class child : public parent{
    public:
    void getInfo(){
        cout<<"Child class \n";
    }
};
int main() {
    parent c1;
    c1.getInfo();
    return 0;
}
